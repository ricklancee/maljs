{
  "name": "maljs",
  "version": "0.0.1",
  "description": "A js json api wrapper for the myanimelist api",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ricklancee/maljs.git"
  },
  "author": "Rick Lancee",
  "bugs": {
    "url": "https://github.com/ricklancee/maljs/issues"
  },
  "scripts": {
    "uglify": "./node_modules/.bin/uglifyjs --compress --mangle -o dist/maljs.min.js -- dist/maljs.js",
    "build": "./node_modules/.bin/browserify src/maljs.js -o dist/maljs.js -t babelify",
    "watch": "nodemon --watch src -x \"npm run build\""
  },
  "homepage": "https://github.com/ricklancee/maljs#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "nodemon": "^1.9.2",
    "uglify-js": "^2.7.0",
    "xml2js": "^0.4.17"
  }
}
