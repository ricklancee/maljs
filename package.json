  },
  "homepage": "https://github.com/ricklancee/maljs#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "chalk": "^1.1.3",
    "uglify-js": "^2.7.4"
  },
  "dependencies": {
    "request": "^2.78.0",
    "xml2js": "^0.4.17"
  }
}
